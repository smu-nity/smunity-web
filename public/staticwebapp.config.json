{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://smunity.co.kr/api/{*rest}"
    },
    {
      "route": "/actuator/info",
      "rewrite": "https://smunity.co.kr/actuator/info"
    },
    {
      "route": "/grafana/*",
      "rewrite": "https://smunity.co.kr/grafana/{*rest}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.css", "/*.js", "/*.png", "/*.jpg", "/*.svg", "/*.ico"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  }
}
